<?xml version="1.0" encoding="UTF-8"?>

<services>
   <service>
      <name>org.xito.splash</name>
      <display-name>Splash Service</display-name>
      <href>xito-splashscreen/xito-splashscreen.srv</href>
   </service>
   
   <!-- Have to list Launcher so that it will launch in Min-Mode -->
   <service>
      <name>org.xito.launcher</name>
      <display-name>Launcher</display-name>
      <minimum-srv>true</minimum-srv>
      <href>xito-launcher/xito-launcher.srv</href>
   </service>
   <!--
   <service>
      <name>org.xito.blx</name>
      <display-name>BLX Component Framework</display-name>
      <href>blx/blx.srv</href>
   </service>
   -->
 
   <service>
      <name>org.xito.appmanager</name>
      <display-name>Xito Application Manager</display-name>
      <href>xito-appmanager/xito-appmanager.srv</href>
   </service>

</services>
